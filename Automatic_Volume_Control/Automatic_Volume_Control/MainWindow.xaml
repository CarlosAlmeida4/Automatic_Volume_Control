<Window x:Class="Automatic_Volume_Control.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Automatic_Volume_Control"
        mc:Ignorable="d"
        Title="MainWindow" Height="272.445" Width="406.296" Opacity="0.75" Background="#FF323232">


    <Grid>
        <StackPanel Height="132" Margin="141,54,144,55" VerticalAlignment="Stretch" Background="#FF36363B">
            <Button x:Name="Mute" Content="Mute" HorizontalAlignment="Left" VerticalAlignment="Top" Width="110" Height="27" Background="#FF5A5AAA" BorderBrush="{x:Null}" Click="Mute_Click"/>
            <Button x:Name="Vol_up" Content="Volume Up"  HorizontalAlignment="Left" VerticalAlignment="Top" Width="110" Height="50" Background="#FF5A5AAA" BorderBrush="{x:Null}" Click="Vol_up_Click"/>
            <Button x:Name="Vol_Dwn" Content="Volume Down" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="110" Height="50" Background="#FF5A5AAA" BorderBrush="{x:Null}" Click="Vol_Dwn_Click"/>
        </StackPanel>
    </Grid>
</Window>
